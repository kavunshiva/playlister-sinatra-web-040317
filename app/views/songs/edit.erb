<form action="/songs/<%= @song.slug %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">
  <label>New song name: </label>
  <input id="Name" type="text" name="song[name]" /> <br/>
  <label>New song artist: </label>
  <input id="Artist Name" type="text" name="song[artist]" /> <br/>
  <label>New artist genres:</label><br/>
  <% @genres.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" <%="checked" if @song.genres.include?(genre)%> name="song[genre_ids][]" value="<%= genre.id %>"/>
    <label><%= genre.name %></label><br/>
  <% end %>
  <input type="submit" value="Save" />
</form>
